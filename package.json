{
  "name": "aipowerhouse-ui",
  "version": "0.1.0",
  "description": "A polished UI shell for orchestrating multi-model AI workflows",
  "private": true,
  "type": "module",
  "main": "dist/server.js",
  "bin": {
    "aipowerhouse": "./dist/scripts/cli.js"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "bash scripts/dev.sh",
    "build": "bash scripts/build.sh",
    "start": "NODE_ENV=production node dist/server.js",
    "lint": "eslint . --ext .ts --max-warnings=0 --no-error-on-unmatched-pattern",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "setup": "bash scripts/setup.sh",
    "docker:build": "bash scripts/docker-build.sh",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "migrate": "bash scripts/migrate.sh",
    "cli": "node dist/scripts/cli.js",
    "clean": "rm -rf dist coverage node_modules",
    "prepare": "husky install || true"
  },
  "keywords": [
    "ai",
    "openai",
    "anthropic",
    "google",
    "gemini",
    "claude",
    "chatgpt",
    "multi-model",
    "orchestration",
    "api",
    "typescript",
    "express"
  ],
  "author": "AiPowerHouse Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aipowerhouse/ui.git"
  },
  "bugs": {
    "url": "https://github.com/aipowerhouse/ui/issues"
  },
  "homepage": "https://aipowerhouse.dev",
  "dependencies": {
    "commander": "^12.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "helmet": "^8.0.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.15.17",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "eslint": "^8.57.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "prettier": "^3.2.5",
    "supertest": "^7.0.0",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
